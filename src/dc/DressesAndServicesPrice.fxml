<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dc.DressesAndServicesPriceController">
   <children>
      <Pane prefHeight="400.0" prefWidth="600.0">
         <children>
            <HBox layoutX="-1.0" layoutY="161.0" prefHeight="112.0" prefWidth="600.0">
               <children>
                  <VBox prefHeight="129.0" prefWidth="203.0">
                     <children>
                        <Label text="Select Dress" />
                        <ComboBox fx:id="dressComboBox" prefHeight="30.0" prefWidth="170.0" />
                     </children>
                     <padding>
                        <Insets left="30.0" />
                     </padding>
                  </VBox>
                  <VBox prefHeight="127.0" prefWidth="196.0">
                     <children>
                        <Label text="Select Service" />
                        <ComboBox fx:id="ServiceComboBox" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="30.0" prefWidth="170.0" />
                     </children>
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                  </VBox>
                  <VBox prefHeight="105.0" prefWidth="66.0">
                     <children>
                        <Label text="Price" />
                        <TextField fx:id="priceTextField" prefHeight="30.0" prefWidth="77.0" />
                     </children>
                  </VBox>
                  <Button fx:id="addButton" mnemonicParsing="false" onAction="#addButtonActionListner" prefHeight="30.0" prefWidth="75.0" text="Add">
                     <HBox.margin>
                        <Insets left="20.0" top="16.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <Label layoutX="162.0" layoutY="54.0" text="Updating Dresses &amp; Services Prices">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Button fx:id="closeButton" layoutX="309.0" layoutY="297.0" mnemonicParsing="false" onAction="#closeButtonActionListner" prefHeight="46.0" prefWidth="94.0" text="Close" />
            <Button fx:id="updateButton" layoutX="197.0" layoutY="297.0" mnemonicParsing="false" onAction="#updateButtonActionListner" prefHeight="46.0" prefWidth="94.0" text="Update" />
         </children>
      </Pane>
   </children>
</AnchorPane>
